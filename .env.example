# ===========================================================================
# Qwen3-TTS Configuration
# ===========================================================================

# Model
MODEL_ID=Qwen/Qwen3-TTS-12Hz-0.6B-Base
PRELOAD_MODEL=true

# Quantization: "" (none), "fp8", "int8" (currently broken with bitsandbytes)
QUANTIZE=fp8

# Performance
TORCH_COMPILE=true
TORCH_COMPILE_MODE=max-autotune
CUDA_GRAPHS=true
IDLE_TIMEOUT=0
REQUEST_TIMEOUT=300
MAX_QUEUE_DEPTH=5

# Audio
AUDIO_CACHE_MAX=256
VOICE_CACHE_MAX=32
TEXT_NORMALIZE=true
VAD_TRIM=true

# Network
GATEWAY_MODE=false
UNIX_SOCKET_PATH=
SSL_KEYFILE=
SSL_CERTFILE=

# System (rarely need to change)
INFERENCE_CPU_CORES=
PYTORCH_CUDA_ALLOC_CONF=expandable_segments:True
LD_PRELOAD=/usr/lib/x86_64-linux-gnu/libjemalloc.so.2
MALLOC_CONF=background_thread:true,dirty_decay_ms:1000,muzzy_decay_ms:0
